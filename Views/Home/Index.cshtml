@{
    ViewData["Title"] = "Index";
}
<!Doctype html>
<html lan="en">
    <head>
        <title>Index</title>
    </head>
    <body>
        <h1>TP 4</h1>
        <a href = '@Url.Action("SelectIndumentaria")'><button>Seleccionar indumentaria</button></a>
        <br>
        @foreach(string item in ViewBag.indumentaria.Keys)
        {
            <div style="font-size: 5rem; text-align: center ;">
            @item:
                <img src="/IMG/@ViewBag.indumentaria[item].Medias">
                <img src="/IMG/@ViewBag.indumentaria[item].Pantalon">
                <img src="/IMG/@ViewBag.indumentaria[item].Camiseta">
            </div>
            <br>
        }
    </body>
</html>